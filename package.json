{
  "name": "emoticons",
  "version": "1.2.6",
  "private": true,
  "homepage": "https://emoticons.ddsv.eu/",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "emoji.json": "^13.1.0",
    "node-sass": "^5.0.0",
    "react": "^17.0.2",
    "react-browser-detection": "^1.2.1",
    "react-clipboard.js": "^2.0.16",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "react-virtualized": "^9.22.3",
    "simplebar-react": "^2.3.0",
    "web-vitals": "^1.1.1"
  },
  "devDependencies": {
    "ftp-deploy": "^2.4.1",
    "open-cli": "^6.0.1",
    "window-resizeto": "0.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "coverage": "npm run test --watchAll=false --coverage",
    "show-coverage": "open-cli ./coverage/lcov-report/index.html",
    "deploy": "npm run test --watchAll=false && npm run build && node scripts/deploy.js",
    "update-emoji": "npm i --save emoji.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "src/animate.js",
      "src/index.js",
      "src/serviceWorker.js"
    ]
  }
}
